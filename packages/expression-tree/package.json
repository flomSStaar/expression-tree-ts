{
  "name": "@flomsstaar/expression-tree",
  "version": "0.1.2",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "type": "module",
  "files": [
    "build"
  ],
  "exports": {
    ".": {
      "default": "./build/index.js",
      "types": "./build/index.d.ts"
    }
  },
  "scripts": {
    "dev": "bun index.ts",
    "lint": "eslint src index.ts",
    "lint:fix": "eslint src index.ts --fix",
